      "scripts": {
        "pre_request_script": null,
        "post_request_script": "console.log(pretty_print(response))\n\n// cookies\nconst c = response.cookies.split(\",\")[0].split(\"\\n\");\n\n// session id\nconst session= c.find((v)=>v.includes(\"session\")).split(\": \")[1]\n\n// access token\nconst access= c.find((v)=>v.includes(\"access\")).split(\": \")[1]\n\n// refresh token\nconst refresh= c.find((v)=>v.includes(\"refresh\")).split(\": \")[1]\n\n// setting env vars\nenv.session_id= session;\nenv.access_token= access;\nenv.refresh_token = refresh;"
      },
      "settings": {
        "use_config_proxy": true,
        "allow_redirects": true,
        "store_received_cookies": true,
        "pretty_print_response_content": true
      }
